# Backend Environment Variables

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# Server
PORT=3000
NODE_ENV=development

# Authentication
SESSION_SECRET=your_session_secret_here
JWT_SECRET=your_jwt_secret_here

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Supabase (Moscownpur RealM Integration)
REALM_SUPABASE_URL=https://your-project.supabase.co
REALM_SUPABASE_ANON_KEY=your_supabase_anon_key_here
REALM_SUPABASE_ACCESS_TOKEN=your_supabase_service_role_key_here

# Firebase (Optional - for file uploads)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MSG_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Frontend URL (for CORS in production)
# FRONTEND_URL=https://your-app.onrender.com
